{
    "name": "kwaadpepper/laravel-omen",
    "description": "Yet another laravel filemanager",
    "license": "MIT",
    "authors": [
        {
            "name": "Jérémy Munsch",
            "email": "contact@jeremydev.ovh",
            "role": "Developper",
            "homepage": "https://jeremydev.ovh"
        }
    ],
    "homepage": "https://omen.jeremydev.ovh/",
    "readme": "README.md",
    "support": {
        "email": "github@jeremydev.ovh",
        "issues": "https://github.com/Kwaadpepper/Omen/issues",
        "wiki": "https://github.com/Kwaadpepper/Omen/wiki",
        "source": "https://github.com/Kwaadpepper/Omen"
    },
    "version": "0.0.1",
    "keywords": [
        "laravel",
        "responsive",
        "filemanager",
        "ftp",
        "s3"
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1",
        "jenssegers/date": "4.*",
        "intervention/image": "2.*"
    },
    "suggest": {
        "ext-gd": "to use GD library for image crop and resize",
        "ext-imagick": "to use Imagick for image crop and resize"
    },
    "autoload": {
        "psr-4": {
            "Kwaadpepper\\Omen\\": "src/app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kwaadpepper\\Omen\\Tests\\": "src/tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kwaadpepper\\Omen\\Providers\\OmenServiceProvider"
            ],
            "aliases": {}
        }
    },
    "require-dev": {
        "orchestra/testbench": "^5.3",
        "dms/phpunit-arraysubset-asserts": "^0.2.0",
        "derekmd/laravel-dusk-firefox": "^1.1",
        "orchestra/testbench-dusk": "^5.2"
    }
}
