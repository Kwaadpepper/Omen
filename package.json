{
    "name": "laravel-omen",
    "version": "0.0.1",
    "author": {
        "name": "Jérémy Munsch",
        "email": "contact@jeremydev.ovh"
    },
    "description": "Laravel Omen Filemanager",
    "license": "MIT",
    "devDependencies": {
        "clean-webpack-plugin": "^3.0.0",
        "coffee-loader": "^0.9.0",
        "coffeescript": "^2.5.1",
        "concurrently": "^5.2.0",
        "cross-env": "^7.0.2",
        "laravel-mix": "^5.0.4",
        "less": "^3.11.1",
        "less-loader": "^5.0.0",
        "node-sass": "^4.13.1",
        "resolve-url-loader": "^3.1.1",
        "sass-loader": "^8.0.2",
        "webpack": "^4.42.1",
        "yarn": "^1.22.4"
    },
    "scripts": {
        "tinymce": "./node_modules/coffeescript/bin/coffee -o resources/js/plugins/tinymce.omen.plugin.min.js -c ./src/resources/coffee/tinymce/omen.plugin.coffee",
        "tinymce-watch": "./node_modules/coffeescript/bin/coffee -w -o resources/js/plugins/tinymce.omen.plugin.min.js -c ./src/resources/coffee/tinymce/omen.plugin.coffee",
        "plugins": "yarn tinymce",
        "plugins-watch": "yarn tinymce && yarn tinymce-watch",
        "dev": "yarn clean && yarn plugins && NODE_ENV=development webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js && rm -rf ./resources/css/__*",
        "watch": "yarn clean && concurrently --kill-others --raw \"yarn plugins-watch\" \"NODE_ENV=development webpack --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js\"",
        "production": "yarn clean && yarn plugins && NODE_ENV=production webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js && rm -rf ./resources/css/__*",
        "clean": "rm -rf ./resources/css/* && rm -rf ./resources/js/* && rm -rf ./resources/fonts/* && rm -rf ./resources/images/*",
        "dist": "yarn production"
    },
    "dependencies": {
        "@mdi/font": "^5.1.45",
        "@shopify/draggable": "1.0.0-beta.9",
        "bezier-easing": "^2.1.0",
        "bootstrap": "^4.4.1",
        "bootstrap-fileinput": "^5.0.8",
        "cropperjs": "^1.5.7",
        "fuzzysearch": "^1.0.3",
        "fuzzyset.js": "^0.0.91",
        "highlight.js": "^10.0.1",
        "jquery": "^3.5.1",
        "jquery-contextmenu": "^2.9.2",
        "jquery-ui": "https://github.com/Kwaadpepper/jquery-ui",
        "jquery-whenall": "^0.1.5",
        "jquery.fancytree": "^2.35.0",
        "js-base64": "^2.6.1",
        "lazyload": "^2.0.0-rc.2",
        "lodash": "^4.17.15",
        "mediaelement": "^4.2.16",
        "pdfjs-dist": "^2.3.200",
        "popper.js": "^1.16.1",
        "progressbar.js": "^1.1.0",
        "script-loader": "^0.7.2",
        "simplebar": "^5.1.0",
        "split.js": "^1.5.11",
        "wheelzoom": "https://github.com/Kwaadpepper/wheelzoom.git"
    }
}
